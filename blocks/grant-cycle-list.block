{.if items}
  <div class="grant-list">
    {.section collection}
      <h4 class="grant-list__title">{title}</h4>
      <h5 class="grant-list__description">{description}</h5>
    {.end}
    <ul class="grant-list__list">
      {.repeated section items}
        <li><a href="{fullUrl}">{title}</a></li>
      {.end}
    </ul>
{.end}
